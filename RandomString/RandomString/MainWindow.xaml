<Window x:Class="RandomString.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="RandomStrings" Height="421" Width="593">
    <Grid>
        <ScrollViewer Name="ScrollViewerBox" HorizontalAlignment="Left" Height="344" Margin="10,37,0,0" VerticalAlignment="Top" Width="276">
            <Grid Name="TextBoxsGrid"/>
        </ScrollViewer>
        <ComboBox Name="ComboBoxCount" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="93" SelectionChanged="ComboBox_SelectionChanged_1"/>
        <TextBox Name="TextBoxResult" HorizontalAlignment="Left" Height="344" Margin="304,37,0,0" AcceptsReturn="True" VerticalScrollBarVisibility="Visible" TextWrapping="Wrap" VerticalAlignment="Top" Width="271"/>
        <Button Name="ButtonGenerate" Content="Generate" HorizontalAlignment="Left" Margin="304,10,0,0" VerticalAlignment="Top" Width="75" Click="ButtonGenerate_Click"/>
    </Grid>
</Window>
